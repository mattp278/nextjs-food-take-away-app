{
  "name": "nextjs-food-take-away-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:deploy": "npx dotenv -e .env.local -- prisma migrate deploy",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.local -- prisma db seed",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build",
    "prisma:format": "npx dotenv -e .env.local -- prisma format"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.6",
    "@next/font": "^13.4.0",
    "@prisma/client": "^4.11.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@sendgrid/mail": "^7.7.0",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.52.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vercel/postgres": "^0.3.1",
    "axios": "^1.4.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "formik": "^2.2.9",
    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "next-redux-wrapper": "^8.1.0",
    "nodemailer": "^6.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "react-redux": "^8.0.5",
    "reduxjs-toolkit-persist": "^7.2.1",
    "stripe": "^12.5.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/seed.ts"
  }
}
